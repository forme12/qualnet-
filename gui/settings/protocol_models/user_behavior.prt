<?xml version="1.0" encoding="ISO-8859-1"?>
<category name="NODE CONFIGURATION">
 <subcategory name="USER BEHAVIOR" class="device">
    <variable name="Enable User Behavior Modelling" key="DUMMY-UBEE-ENABLED" type="Selection" default="NO" help="Enables user behavior modelling parameters.">
        <option value="NO" name="No" />
        <option value="YES" name="Yes">
            <variable name="User Profile" key="USER-PROFILE" type="Text" default="[Required]" help="Assigns a user profile to one or more nodes."  invisible="ScenarioLevel"/>
            <variable name="Number of User Status" key="DUMMY-USER-PROFILE-NUMBER" default="1" type="Array">
            <variable name="User Status" key="USER-STATUS" type="Text" default="[Required]" help="Specifies user status. Status name should match a corresponding entry in the user profile." format="List" />
            <variable name="User Status Start Time" key="USER-STATUS-START-TIME" type="Time" default="0S" help="Specifies time at which the user starts to behave with the status defined in this index." format="List" />
            </variable>
            <variable name="User Profile Files" key="DUMMY-HELP-MSG-USER-PROFILE-FILE" type="text" default="Specify User Profile Files in Supplemental Files Tab in Scenario Properties Window." keyvisible="false" disable="true"/>
            <variable name="Traffic Pattern Files" key="DUMMY-HELP-MSG-TRAFFIC-PATTERN-FILE" type="text" default="Specify Traffic Pattern Files in Supplemental Files Tab in Scenario Properties Window." keyvisible="false" disable="true"/>
            <variable name="Arbitrary Distribution Files" key="DUMMY-HELP-MSG-ARBITRARY-DISTRIBUTION-FILE" type="text" default="Specify Arbitrary Distribution Files in Supplemental Files Tab in Scenario Properties Window." keyvisible="false" disable="true"/>
        </option>
    </variable>
</subcategory>
</category>
