<?xml version="1.0" encoding="ISO-8859-1"?>
<category name="NODE CONFIGURATION">
    <subcategory name="INTERFACES">
        <subcategory name="Interface 0"> 
            <variable name="Interface Name" key="INTERFACE-NAME" type="Text" default="interface" help="" interfaceindex="true" optional="true"/>
            <variable name="Interface Type" key="INTERFACE-TYPE" type="Selection" default="DEFAULT" help="" optional="true" interfaceindex="true">
               <option name="DEFAULT_TYPE" value="DEFAULT" />
               <option name="ASYNC" value="ASYNC" />
               <option name="ATM" value="ATM" />
               <option name="BRI" value="BRI" />
               <option name="BVI" value="BVI" />
               <option name="CABLE" value="CABLE" />
               <option name="CBR" value="CBR" />
               <option name="DIALER" value="DIALER" />
               <option name="ETHERNET" value="ETHERNET" />
               <option name="FDDI" value="FDDI" />
               <option name="GROUP_ASYNC" value="GROUP_ASYNC" />
               <option name="HSSI" value="HSSI" />
               <option name="LEX" value="LEX" />
               <option name="LOOPBACK" value="LOOPBACK" />
               <option name="NULL_INTERFACE" value="NULL_INTERFACE" />
               <option name="PORT_CHANNEL" value="PORT_CHANNEL" />
               <option name="SERIAL" value="SERIAL" />
               <option name="TOKENRING" value="TOKENRING" />
               <option name="TUNNEL" value="TUNNEL" />
               <option name="VIRTUAL_TEMPLATE" value="VIRTUAL_TEMPLATE" />
               <option name="VIRTUAL_TOKENRING" value="VIRTUAL_TOKENRING" />
               <option name="VLAN" value="VLAN" />    
            </variable>
            <variable name="Interface Number" key="INTERFACE-NUMBER" type="Text" default="DEFAULT_NUMBER" help="" optional="true" keyvisible="false" />
            <variable name="Configure as Unnumbered Interface" key="UNNUMBERED" type="Checkbox" default="NO" help="Select Yes to configure Unnumbered Interface" interfaceindex="true" optional="true"/>
            <subcategory name="Physical Layer" refpath="NODE CONFIGURATION+Physical Layer" icon="protocol.gif" />
            <subcategory name="MAC Layer" refpath="NODE CONFIGURATION+MAC Layer" icon="protocol.gif" />
            <subcategory name="Network Layer" refpath="NODE CONFIGURATION+Network Layer" icon="protocol.gif" />
            <subcategory name="Routing Protocol" refpath="NODE CONFIGURATION+ROUTING PROTOCOL" icon="protocol.gif" />
            <subcategory name="Faults" refpath="NODE CONFIGURATION+INTERFACE FAULTS" icon="protocol.gif" />
            <subcategory name="File Statistics" refpath="NODE CONFIGURATION+STATISTICS" icon="protocol.gif" help="All the statistics are compiled together into a file called &quot;ExperimentName.stat&quot;(where &lt;br&gt;experiment name is specified on the General settings) at the end of the simulation."/>            
        </subcategory>
    </subcategory>
</category>
