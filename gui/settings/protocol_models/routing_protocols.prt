                                    <option value="NO" name="No" />
                                    <option value="YES" name="Yes">
                                         <variable name="PIM-SM Switch To SPT Threshold" key="ROUTING-PIMSM-SWITCH-SPT-THRESHOLD" type="Text" default="1 GROUP-RANGE &lt;224.0.0.0,15.255.255.255 &gt;"/>
                                    </option>
                                </variable>
                                <variable name="PIM-SM Static RP Present" key="DUMMY-PIM-SM-STATIC-NUMBER-OF-RP" type="Selection" default="NO" optional="true" >
                                <option value="NO" name="No">
                                    <variable name="PIM-SM Candidate Rendezvous Point" key="PIM-SM-CANDIDATE-RP" type="Selection" default="NO" optional="true" >
                                        <option value="NO" name="No" />
                                        <option value="YES" name="Yes">
                                            <variable name="PIM-SM Candidate RP Timeout Interval" key="ROUTING-PIMSM-CANDIDATE-RP-TIMEOUT" type="Time" default="60S" />
                                            <variable name="PIM-SM Candidate RP Priority" key="PIM-SM-CANDIDATE-RP-PRIORITY" type="Integer" default="192" min="0" max="255"/>
                                            <variable name="PIM-SM Candidate RP Group Range" key="PIM-SM-RP-GROUP-RANGE" type="Text" default="&lt;224.0.0.0,15.255.255.255 &gt;"/>
                                        </option>
                                    </variable>
                                        <variable name="PIM-SM Candidate Bootstrap Router" key="PIM-SM-CANDIDATE-BSR" type="Selection" default="NO" optional="true" >
                                            <option value="NO" name="No" />
                                            <option value="YES" name="Yes">
                                                <variable name="PIM-SM Candidate BSR Priority" key="PIM-SM-CANDIDATE-BSR-PRIORITY" type="Integer" default="64" min="0" max="255"/>
                                                <variable name="PIM-SM Bootstrap Period Interval" key="ROUTING-PIMSM-BOOTSTRAP-PERIOD" type="Time" default="60S" />
                                               </option>
                                        </variable>
                                    </option>
                                    <option value="YES" name="Yes">
                                        <variable name="PIM-SM Number of Static RPs" key="PIM-SM-STATIC-NUMBER-OF-RP" default="1" type="Array" optional="true" >
                                            <variable name="PIM-SM Static RP Address" key="PIM-SM-STATIC-RP-ADDRESS" type="Dotted decimal"/>
                                            <variable name="PIM-SM Static RP Priority" key="PIM-SM-STATIC-RP-PRIORITY" type="Integer" default="192" min="0" max="255"/>
                                            <variable name="PIM-SM Static RP Group Range" key="PIM-SM-STATIC-RP-GROUP-RANGE" type="Text" default="&lt;224.0.0.0,15.255.255.255 &gt;"/>
                                        </variable>
                                    </option>
                                </variable>
                                <variable name="PIM-SM Triggered Delay" key="ROUTING-PIMSM-TRIGGERED-DELAY" type="Time" default="30S" />
                                <variable name="PIM-SM Keep-alive Timeout Interval" key="ROUTING-PIMSM-KEEPALIVE-TIMEOUT" type="Time" default="210S" />
                                <variable name="PIM-SM Join/Prune Hold Timeout Interval" key="ROUTING-PIMSM-JOINPRUNE-HOLD-TIMEOUT" type="Time" default="210S" />
                                <variable name="PIM-SM T Periodic Interval" key="ROUTING-PIMSM-T-PERIODIC-INTERVAL" type="Time" default="60S" />
                                <variable name="PIM-SM Assert Timeout Interval" key="ROUTING-PIMSM-ASSERT-TIMEOUT" type="Time" default="180S" />
                                <variable name="PIM-SM Registered Supression Time" key="ROUTING-PIMSM-REGISTER-SUPRESSION-TIME" type="Time" default="60S" />
                                <variable name="PIM-SM Register Probe Time" key="ROUTING-PIMSM-REGISTER-PROBE-TIME" type="Time" default="5S" />
                            </option>
                        </variable>
                    </option>
                    <option value="MAODV" name="MAODV" help="MAODV follows manet-maodv-00.txt." addon="maodv">
                        <variable name="Network Diameter (hops)" key="MAODV-NET-DIAMETER" type="Integer" default="35" help="The maximum possible number of hops between two nodes in the network" />
                        <variable name="Node Traversal Time Interval" key="MAODV-NODE-TRAVERSAL-TIME" type="Time" default="40MS" help="Conservative estimate of the average one hop traversal time for packets and should include queuing, transmission, propagation and other delays" />
                        <variable name="Active Route Timeout Interval" key="MAODV-ACTIVE-ROUTE-TIMEOUT" type="Time" default="3S" help="Timeout time for an active route; each time a data packet is received, the lifetime of that route is updated to this value.  A default value of 10 seconds is suggested for error detection through MAC layer message (like what 802.11 does)" />
                        <variable name="My Route Timeout Interval" key="MAODV-MY-ROUTE-TIMEOUT" type="Time" default="6S" help="The destination of a RREQ replies with MAODV-MY-ROUTE-TIMEOUT as the lifetime of the route." />
                        <variable name="Hello Interval" key="MAODV-HELLO-INTERVAL" type="Time" default="1S" help="Lifetime of a hello message is determined by MAODV-ALLOWED-HELLO-LOSS * MAODV-HELLO-INTERVAL" />
                        <variable name="Group Hello Interval" key="MAODV-GROUP-HELLO-INTERVAL" type="Time" default="5S" help="Lifetime of a group hello message" />
                        <variable name="Maximum Allowed Hello Loss" key="MAODV-ALLOWED-HELLO-LOSS" type="Integer" default="2" min="2" max="3" help="Lifetime of a hello message is determined by MAODV-ALLOWED_HELLO_LOSS * MAODV-HELLO_INTERVAL" />
                        <variable name="Maximum RREQ Retries" key="MAODV-RREQ-RETRIES" type="Integer" default="2" min="0" help="Specifies the number of times MAODV will repeat expanded ring search for a destination if no Route Reply is received within specified amount of time." />
                        <variable name="Route Deletion Constant" key="MAODV-ROUTE-DELETION-CONSTANT" type="Integer" default="5" help="A constant use for calculating the time after which an active route should be deleted.  After timeout of an active route, the route is finally deleted from the routing table after a time period of (K * max (MAODV-ACTIVE_ROUTE_TIMEOUT,MAODV-ALLOWED_HELLO_LOSS * MAODV-HELLO_INTERVAL)), Here K is MAODV-ROUTE-DELETION-CONSTANT." />
                        <variable name="Maximum Number of Buffered Packets" key="MAODV-BUFFER-MAX-PACKET" type="Integer" default="100" help="Maximum number of packets the message buffer of MAODV can hold. If the buffer fills up, incoming packets for the buffer will be dropped." />
                        <variable name="Maximum Buffer Size (bytes)" key="MAODV-BUFFER-MAX-BYTE" type="Integer" default="0" help="If nothing is specified, buffer overflow will be checked by number of packets in the buffer. If some value is specified here, incoming packets will be dropped if the incoming packet size + current size of the buffer exceeds this value." />
                        <variable name="TTL Start" key="MAODV-TTL-START" type="Integer" default="1" min="1" />
                        <variable name="TTL Increment" key="MAODV-TTL-INCREMENT" type="Integer" default="2" min="1" />
                        <variable name="TTL Threshold" key="MAODV-TTL-THRESHOLD" type="Integer" default="7" requires="this.value > [MAODV-TTL-START]" />
                    </option>
                </variable>
            </option>
        </variable>
        <variable name="Configure Default Gateway" key="DUMMY-GATEWAY-CONFIGURATION" type="Selection" default="NO" invisible="scenariolevel,interface" optional="true" >
            <option value="NO" name="No" />
            <option value="YES" name="Yes">
                <variable name="Default Gateway" key="DEFAULT-GATEWAY" type="text" default="[Required]" />
            </option>
        </variable>
        <variable name="Enable HSRP Protocol" key="HSRP-PROTOCOL" type="Selection" default="NO" addon="multimedia_enterprise" optional="true" >
            <option value="YES" name="Yes">
                <variable name="Standby Group Number" key="HSRP-STANDBY-GROUP-NUMBER" type="Integer" default="0" />
                <variable name="Virtual IP Address" key="HSRP-VIRTUAL-IP-ADDRESS" type="Text" default="0.0.10.1" />
                <variable name="Priority" key="HSRP-PRIORITY" type="Integer" default="0" />
                <variable name="Hello Time" key="HSRP-HELLO-TIME" type="Time" default="3S" />
                <variable name="Hold Time" key="HSRP-HOLD-TIME" type="Time" default="10S" />
                <variable name="Enable Preemption Capability" key="HSRP-PREEMPTION-CAPABILITY" type="Checkbox" default="NO"/>
            </option>
            <option value="NO" name="No" />
        </variable>
        <subcategory name="BGP Configuration" class="device,atmdevice">
            <variable name="Set BGP Protocol Parameters" key="DUMMY-BGP" type="Selection" default="NO" addon="multimedia_enterprise" invisible="interface,WiredSubnet, WirelessSubnet" optional="true" >
                <option value="NO" name="No" />
                <option value="YES" name="Yes">
                    <variable name="Enable Legacy Support" key="BGP-NEXT-HOP-LEGACY-SUPPORT" type="Selection" default="YES" help="If the value is set to NO, a node will not modify the next-hop after receiving an UPDATE message from an external peer." optional="true" >
                        <option value="NO" name="No" />
                        <option value="YES" name="Yes"/>
                    </variable>
                    <variable name="Start Time" key="BGP-START-TIME" type="Time" default="0S" help="The amount of time a speaker will wait before trying to open TCP connection with a peer"/>               
                    <variable name="Hold Time Interval" key="BGP-HOLD-TIME-INTERVAL" type="Time" default="90S" help="The amount of time a speaker will wait to listen for activities from a peer"/>
                    <variable name="Large Hold Time Interval" key="BGP-LARGE-HOLD-TIME-INTERVAL" type="Time" default="4M" help="The hold time in active state"/>
                    <variable name="Minimum External Route Advertisement Interval" key="BGP-MIN-RT-ADVERTISEMENT-INTERVAL-EBGP" type="Time" default="30S" help="The interval between two subsequent update messages for external peers"/>
                    <variable name="Minimum Internal Route Advertisement Interval" key="BGP-MIN-RT-ADVERTISEMENT-INTERVAL-IBGP" type="Time" default="5S" help="The interval between two subsequent update messages for internal peers"/>
                    <variable name="Minimum AS Origination Interval" key="BGP-MIN-AS-ORIGINATION-INTERVAL" type="Time" default="15S" help="The interval between two subsequent update messages for internal peers advertising IGP routes"/>
                    <variable name="Keep-alive Interval" key="BGP-KEEPALIVE-INTERVAL" type="Time" default="30S" help="The interval between two successive keep alive message"/>
                    <variable name="Connect Retry Interval" key="BGP-CONNECT-RETRY-INTERVAL" type="Time" default="120S" help="The amount of time to wait before re-opening a TCP connection"/>
                    <variable name="Route Waiting Interval" key="BGP-ROUTE-WAITING-INTERVAL" type="Time" default="15S" help="The amount of time to wait to determine if a neighbor is no longer reachable"/>
                </option>
            </variable>
            <variable name="Local Preference" key="DEFAULT-LOCAL-PREFERENCE" type="Integer" default="100" keyvisible="false" invisible="scenariolevel,WiredSubnet,WirelessSubnet,interface"/>
            <variable name="Multi-exit Discriminator" key="DEFAULT-METRIC" type="Integer" default="0" keyvisible="false" invisible="scenariolevel,WiredSubnet, WirelessSubnet,interface"/>
            <variable name="Use IGP Route Synchronization" key="NO SYNCHRONIZATION" type="Checkbox" default="YES" keyvisible="false" invisible="scenariolevel,WiredSubnet,WirelessSubnet,interface"/>
            <variable name="Stop Route Injection from IGP" key="NO-ADVERTISEMENT-FROM-IGP" type="Selection" default="NO" keyvisible="false" invisible="scenariolevel,WiredSubnet,WirelessSubnet,interface" optional="true" >
                <option value="NO" name="No">
                    <variable name="IGP Probe Interval" key="BGP-PROBE-IGP-INTERVAL" type="Time" default="10M" keyvisible="false" invisible="scenariolevel,WiredSubnet,WirelessSubnet,interface"/>
                </option>
                <option value="YES" name="Yes"/>
            </variable>
            <variable name="Stop Route Distribution to OSPF" key="NO-REDISTRIBUTION-TO-OSPF" type="Selection" default="NO" keyvisible="false" invisible="scenariolevel,WiredSubnet,WirelessSubnet,interface" optional="true" >
                <option value="NO" name="No"/>
                <option value="YES" name="Yes"/>
            </variable>
            <variable name="Stop Static And Default Route Distribution to BGP" key="BGP-NO-REDISTRIBUTE-STATIC-CONNECTED-ROUTE" type="Selection" default="NO" keyvisible="false" invisible="scenariolevel,WiredSubnet,WirelessSubnet,interface" optional="true" >
                <option value="NO" name="No"/>
                <option value="YES" name="Yes"/>
            </variable>
            <subcategory name="BGP Router-Id">
                <variable name="Enable BGP Router-Id" key="BGP-ENABLE-ROUTER-ID" type="Selection" default="NO" visibilityrequires="[NETWORK-PROTOCOL] != 'IPv6'">
                    <option value="NO" name="No"/>
                    <option value="YES" name="Yes">
                        <variable name="BGP Router-Id" key="BGP ROUTER-ID" type="Dotted decimal" default="127.0.0.1" min="127.0.0.1"/>
                    </option>
                </variable>
                <variable name="Enable BGP Router-Id IPv6" key="BGP-ENABLE-ROUTER-ID_IPv6" type="Selection" default="YES" visibilityrequires="[NETWORK-PROTOCOL] == 'IPv6'">
                    <option value="YES" name="Yes">
                        <variable name="BGP Router-Id" key="BGP ROUTER-ID" type="Dotted decimal" default="127.0.0.1" min="127.0.0.1" max="127.255.255.254" />
                    </option>
                </variable>
            </subcategory>
            <subcategory name="Networks Advertised">
                <variable name="Network Address" key="NETWORK" type="NetworkCheckBox" default="" keyvisible="false" invisible="scenariolevel,subnet,interface"/>
            </subcategory>
            <subcategory name="Neighbours">
                <subcategory name="Neighbour 0">
                    <variable name="Neighbour Address" key="NEIGHBOR" type="SelectionDynamic" default="" keyvisible="false" invisible="scenariolevel,subnet,interface"/>
                    <variable name="Weight Assigned" key="WEIGHT" type="Integer" default="32767" keyvisible="false" invisible="scenariolevel,subnet,interface"/>
                    <variable name="Multi-exit Discriminator" key="DEFAULT-METRIC" type="Integer" default="0" keyvisible="false" invisible="scenariolevel,subnet,interface"/>
                    <variable name="Set Route Reflector Client"  key="ROUTE-REFLECTOR-CLIENT" type="Selection" default="NO" keyvisible="false" invisible="scenariolevel,interface" optional="true" >
                        <option value="NO" name="No" /> 
                        <option value="YES" name="Yes"/> 
                     </variable>
                </subcategory>
            </subcategory>
        </subcategory>
    </subcategory>
</category>
