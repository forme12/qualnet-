                    <option value="USER-SPECIFIED" name="User-Specified">
                        <variable key="LINK-11-USER-SPECIFIED-POLLING-ORDER" type="Text" name="Polling List" default="{}" optional="true" help="List the nodes which are ICMP routers between the '{}' separated by commas.  You can use 'thru' to specify a range of nodes." invisible="Device" />
                    </option>
                </variable>
                <variable name="Configure Link 11 Rx Threshold Manully" key="USER-SPECIFIED-RX-THRESHOLD" type= "Selection" default="NO">
                    <option value="NO" name="No" />
                    <option value="YES" name="Yes">
                        <variable name="Link 11 Rx Threshold" key="LINK-11-RX-SNR-THRESHOLD" type= "Fixed" default="10"/>
                    </option>
                </variable>
            </option>
            <option value="MODE5" name="Mode 5 MAC" addon="jne"/>
            <option value="GENERICMAC" name="Generic MAC" visibilityrequires="[DUMMY-INTERFACE-TYPE] == 'SUBNET-WIRELESS'">
                <variable name="Channel Access Mode" key="CHANNEL-ACCESS" type="Selection" default="PSMA">
                    <option value="PSMA" name="PSMA" />
                    <option value="CSMA" name="CSMA" />
                    <option value="CSMA-CA" name="CSMA-CA" />
                    <option value="SLOT-CSMA" name="SLOT-CSMA">
                        <variable name="Slot Duration" key="SLOT" type="Time" default="10MS" help="GENERICMAC slot duration must be greater than or equal to 1MS and less than or equal to 1S. The default value is 10MS. This duration should be longer enough so that a packet can be transmitted." />
                    </option>
                    <option value="SLOT-CSMA-CA" name="SLOT-CSMA-CA">
                        <variable name="Slot Duration" key="SLOT" type="Time" default="10MS" help="GENERICMAC slot duration must be greater than or equal to 1MS and less than or equal to 1S. The default value is 10MS. This duration should be longer enough so that a packet can be transmitted." />
                    </option>
                </variable>
                <variable name="Enable RTS/CTS" key="RTS-CTS" type="Checkbox" default="NO" />
                <variable name="Enable ACK" key="ACK" type="Checkbox" default="NO" />
                <variable name="Retry Limit" key="RETRY-LIMIT" type="Integer" default="25" min="0"/>
            </option>
            <option value="USAP" name="USAP" addon="military" invisible="WiredSubnet" visibilityrequires="[DUMMY-INTERFACE-TYPE] == 'SUBNET-WIRELESS'">
                <variable name="Sync. Slot Duration" key="USAP-SYNC-SLOT-DURATION" type="Time" default="2MS" min="0S" help="Duration of a synchronization slot." />
                <variable name="Bootstrap Slot Duration" key="USAP-BOOTSTRAP-SLOT-DURATION" type="Time" default="3MS" min="0S" help="Duration of a bootstrap slot." />
                <variable name="Data Slot Duration" key="USAP-SLOT-DURATION" type="Time" default="8MS" min="0S" help="Duration of a broadcast or reserved slot." />
                <variable name="Number of Sync. Slots per Frame" key="USAP-SYNC-SLOT-PER-FRAME" type="Integer" default="3" min="0" max="65535" help="Number of synchronization slots in one frame." />
                <variable name="Number of Bootstrap Slots per Frame" key="USAP-BOOTSTRAP-SLOT-PER-FRAME" type="Integer" default="13" min="1" max="65535" help="Number of bootstrap slots in one frame." />
                <variable name="Number of Broadcast Slots per Frame" key="USAP-BROADCAST-SLOT-PER-FRAME" type="Integer" default="2" min="1" max="65535" help="Number of broadcast slots in one frame." />
                <variable name="Number of Reserved Slots per Frame" key="USAP-RESERVED-SLOT-PER-FRAME" type="Integer" default="8" min="1" max="65535" help="Number of reserved slots in one frame." />
                <variable name="Number of Frames per Bootstrap Cycle" key="USAP-BOOTSTRAP-CYCLE" type="Integer" default="4" min="1" max="65535" help="Number of frames in one bootstrap cycle." />
                <variable name="Number of Frames per Broadcast Cycle" key="USAP-BROADCAST-CYCLE" type="Integer" default="4" min="1" max="65535" help="Number of frames in one broadcast cycle." />
                <variable name="Maximum Number of Broadcast Cycles per Epoch" key="USAP-BROADCAST-EPOCH-SIZE" type="Integer" default="0" min="0" max="65535" optional="true" help="Maximum number of broadcast cycles in one broadcast epoch." />
                <variable name="Inter-slot Guard Time" key="USAP-GUARD-TIME" type="Time" default="0MS" help="Guard time between two consecutive slots in a frame." />
                <variable name="Inter-frame Guard Time" key="USAP-INTER-FRAME-TIME" type="Time" default="0MS" help="Guard time between two consecutive frames." />
                <variable name="Enable Channelized Neighborhood" key="USAP-CHANNELIZED-NEIGHBORHOOD" type="Checkbox" default="YES" help="Whether to utilize channelized neighborhood." />
                <variable name="Enable Adaptive Bootstrap Cycle" key="USAP-ADAPTIVE-BOOTSTRAP-CYCLE" type="Checkbox" default="NO" help="Whether to adaptively assign bootstrap slots." />
                <variable name="Wait Confirmation for Slot Reservation" key="USAP-WAIT-ASSIGNMENT-CONFIRMATION" type="Checkbox" default="NO" help="Whether to wait for confirmation of the slot reservation before transmitting." />
                <variable name="Data Packet Fragmentation Unit Size (bytes)" key="USAP-FRAGMENTATION-UNIT" type="Integer" default="2048" min="512" max="65535" optional="true" help="Unit size for fragmenting data packets." />
                <variable name="Neighbor Timeout Value" key="USAP-NEIGHBOR-LIFETIME" type="Integer" default="3" min="2" max="65535" help="The time out interval of a neighbor from neighbor list in the unit of # of bootstrap cycles." />
                <variable name="Maximum Slot Idle Duration" key="USAP-MAX-SLOT-IDLE-TIME" type="Time" default="10S" help="Maximum idle time of a reserved slot. Once exceeded, the slot will be freed." />
                <variable name="Maximum MAC Buffer Size (bytes)" key="USAP-PACKET-BUFFER-SIZE" type="Integer" default="500000" min="5000" help="Maximum size of the MAC layer buffer in terms of number of bytes." />
                <variable name="Number of Buckets for Traffic Monitoring" key="USAP-TRAFFIC-BUCKET-MONITOR-INTERVAL" type="Integer" default="64" help="Number of buckets used for monitoring the traffic rate per neighbor." />
                <variable name="Maximum Number of Slots for One Neighbor" key="USAP-MAX-RESERV-SLOT-PER-NEIGHBOR" type="Integer" default="2" min="1" help="Maximum number of slots that can be reserved for transmitting to one neighbor." />
            </option>
            <option value="MAC-WORMHOLE" name="Wormhole" addon="cyber" visibilityrequires="[DUMMY-INTERFACE-TYPE] == 'SUBNET-WIRELESS'">
                <variable name="Wormhole Operation Mode" key="WORMHOLE-MODE" type="Selection" default="THRESHOLD">
                    <option value="THRESHOLD" name="Threshold">
                        <variable name="Wormhole Link Drop Threshold" key="WORMHOLE-THRESHOLD" type="Integer" default="150" />
                    </option>
                    <option value="ALLPASS" name="All Pass"/>
                    <option value="ALLDROP" name="All Drop"/>
                </variable>
                <variable name="Wormhole Link Bandwidth (bits/second)" key="WORMHOLE-LINK-BANDWIDTH" type="Fixed multiplier" default="10000000 bps" unit="bps" maxunit="Ebps" minunit="bps"/>
                <variable name="Wormhole Replay Mac Protocol" key="WORMHOLE-REPLAY-MAC-PROTOCOL" type="Text" default="WORMHOLE-CSMA" disable="true" />
                <variable name="Wormhole Propagation Delay" key="WORMHOLE-PROPAGATION-DELAY" type="Time" default="1US" />
                <variable name="Specify Wormhole Victim Turnaround Time" key="WORMHOLE-VICTIM-COUNT-TURNAROUND-TIME" type="Selection" default="NO" >
                    <option value="NO" name="No" />
                    <option value="YES" name="Yes">
                        <variable name="Wormhole Victim Turnaround Time" key="WORMHOLE-VICTIM-TURNAROUND-TIME" type="Time" default="0MS" />
                    </option>
                </variable>
            </option>
        </variable>
        <variable name="MAC Propagation Delay" key="MAC-PROPAGATION-DELAY" type="Time" default="1US" help="specifies an additional delay for messages sent by the MAC layer to the phy layer.  Some MAC protocols use a multiple of this value." addon="wireless" visibilityrequires="[MAC-PROTOCOL] != 'None'"/>
        <variable name="Enable Promiscuous Mode" key="PROMISCUOUS-MODE" type="Checkbox" default="NO" optional="true" help="must be set to YES if nodes want to overhear packets destined to the neighboring node." />
        <variable name="Enable LLC" key="LLC-ENABLED" type="Checkbox" default="NO" invisible="device, ScenarioLevel"/>
        <variable name="Configure MAC Address" key="DUMMY-MAC-ADDRESS" help="ARP must be enabled to configure MAC address." type="Selection" default="NO" optional="true" invisible="ScenarioLevel, Device, WirelessSubnet">
            <option value="NO" name="No"></option>
            <option value="YES" name="Yes" >
                <variable name="Read MAC Address from File" key="DUMMY-ADDRESS-FROM-FILE" type="Selection" default="NO" keyvisible="false" >
                    <option name="Yes" value="YES">
                        <variable name="MAC Address Configuration File" key="MAC-ADDRESS-CONFIG-FILE" type="File" default="[Required]" filetype="mac-address"/>    
                    </option>
                    <option name="No" value="NO">
                        <variable name="MAC Address Type" key="MAC-ADDRESS-TYPE" type="Selection" default="ETHERNET">
                            <option value="ETHERNET" name="Ethernet"/>
                            <option value="ATM" name="ATM"/>
                        </variable>
                        <variable name="MAC Address Value" key="MAC-ADDRESS" type="Text" default="[Required]"/>
                        <variable name="MAC Address Length" key="MAC-ADDRESS-LENGTH" type="Integer" default="6" disable="true"/>    
                    </option>
                </variable>
            </option>
        </variable>    
        <variable key="SWITCH-STATION-VLAN-TAGGING" type="Selection" name="Use Station VLAN Tagging" default="NO" optional="true" visibilityrequires="[DUMMY-INTERFACE-TYPE] == 'LINK-WIRED' || [DUMMY-INTERFACE-TYPE] == 'LINK-WIRELESS' || [DUMMY-INTERFACE-TYPE] == 'LINK-MICROWAVE' || [DUMMY-INTERFACE-TYPE] == 'SUBNET-WIRED'" >
            <option value="NO" name="No" />
            <option value="YES" name="Yes" >
                <variable key="SWITCH-STATION-VLAN-ID" type="Integer" name="Station VLAN ID" default="1" min="0" max="4090" />
            </option>
        </variable>
    </subcategory>
</category>
