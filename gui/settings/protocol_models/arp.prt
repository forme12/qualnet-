<?xml version="1.0" encoding="ISO-8859-1"?>
<category name="NODE CONFIGURATION">
    <subcategory name="ARP" class="interface,device,atmdevice">
        <variable name="Enable ARP" key="ARP-ENABLED" type="Selection" default="NO" optional="true" invisilbe="interface">
            <option value="NO" name="No" />
            <option value="YES" name="Yes">
                <variable name="Number of Buffer Entries" key="ARP-BUFFER-SIZE" type="Integer" default="1" optional="true" help="Number of entries in the buffer" />
                <variable name="ARP Timeout Interval" key="ARP-CACHE-EXPIRE-INTERVAL" type="Time" default="20M"/>
                <variable name="ARP Static Cache File" key="ARP-STATIC-CACHE-FILE" type="File" default="[Optional]" filetype="arp-static" />
                <!--<variable name="MAC Address Configuration File" key="MAC-ADDRESS-CONFIG-FILE" type="File" default="[Optional]" filetype="mac-address" />-->
                <!--<variable name="ARP Statistics" key="ARP-STATISTICS" type="TickBox" default="YES" /> -->
            </option>
        </variable>
    </subcategory>
</category>
