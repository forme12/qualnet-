                <option value="YES" name="Yes">
            <variable name="IAHEP Node Type" key="IAHEP-NODE-TYPE" type="Selection" default="N/A" addon="cyber,ces" invisible="Interface, WirelessSubnet">
                <option value="IAHEP" name="IAHEP">
                    <variable name="IAHEP AMD File" key="IAHEP-AMD-FILE" type="File" default="[Required]" filetype="amd" invisible="Interface, WirelessSubnet"/>
                    <variable name="IAHEP IGMP Mode" key="IAHEP-IGMP-MODE" type="Selection" default="NONE" invisible="Interface, WirelessSubnet">
                    	<option value="BYPASS" name="Bypass" />
                	<option value="NONE" name="None"/>
                    </variable>
                </option>
                <option value="RED" name="RED" />
                <option value="BLACK" name="BLACK"/>
                    </variable>
                <variable name="Encapsulation Overhead Size (bytes)" key="IAHEP-ENCAPSULATION-OVERHEAD-SIZE" type="Integer" default="0" invisible="Interface, WirelessSubnet"/>
