                        <variable name="HMAC-SHA-1 Rate (bps)" key="IPSEC-HMAC-SHA-1-PROCESSING-RATE" type="Fixed" default="800000000" />
                        <variable name="HMAC-SHA-1-96 Rate (bps)" key="IPSEC-HMAC-SHA-1-96-PROCESSING-RATE" type="Fixed" default="800000000" />
                    </option>
                </variable>
