                </option>
            </variable>
            <variable name="IAHEP Interface Type" key="IAHEP-INTERFACE-TYPE" type="Selection" default="N/A" addon="cyber,ces" visibilityrequires="[IAHEP-NODE-TYPE] != 'N/A' &amp;&amp; [DUMMY-INTERFACE-TYPE] == 'LINK-WIRED'" invisible="Device">
