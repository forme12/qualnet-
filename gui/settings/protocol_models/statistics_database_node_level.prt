<?xml version="1.0" encoding="ISO-8859-1"?>
<category name="NODE CONFIGURATION">

    <subcategory name="STATISTICS DATABASE NODE LEVEL">	
	<variable name="Specify Meta Data for Description Tables" key="DUMMY-META-DATA-FOR-DESCRIPTION-TABLES" type="Selection" default="NO">
            <option value="NO" name="No" />
            <option value="YES" name="Yes">
	        <variable name="Number of Node Meta-data Columns" key="GUI-NUM-NODE-META-DATA" type="Array" default="0" min="0" keyvisible="true" invisible="Interface">
                     <variable name="Node Meta-data" key="NODE-META-DATA" type="Text" default="" />
                </variable>
                <variable name="Number of Session Meta-data Columns" key="GUI-NUM-SESSION-META-DATA" type="Array" default="0" min="0" keyvisible="true" invisible="Interface">
                    <variable name="Session Meta-data" key="SESSION-META-DATA" type="Text" default="" />
                </variable>
                <variable name="Number of Connection Meta-data Columns" key="GUI-NUM-CONNECTION-META-DATA" type="Array" default="0" min="0" keyvisible="true" invisible="Interface">
                    <variable name="Connection Meta-data" key="CONNECTION-META-DATA" type="Text" default="" />
                </variable>
                <variable name="Number of Interface Meta-data Columns" key="GUI-NUM-INTERFACE-META-DATA" type="Array" default="0" min="0" keyvisible="true" invisible="Interface">
                    <variable name="Interface Meta-data" key="INTERFACE-META-DATA" type="Text" default="" />
                </variable>
                <variable name="Number of Scheduler Meta-data Columns" key="GUI-NUM-SCHEDULER-META-DATA" type="Array" default="0" min="0" keyvisible="true" invisible="Interface">
                    <variable name="Scheduler Meta-data" key="SCHEDULER-META-DATA" type="Text" default="" />
                </variable>
                <variable name="Number of Queue Meta-data Columns" key="GUI-NUM-QUEUE-META-DATA" type="Array" default="0" min="0" keyvisible="true" invisible="Interface">
                    <variable name="Queue Meta-data" key="QUEUE-META-DATA" type="Text" default="" />
                </variable>
            </option>
       </variable>
       <variable name="Configure Model-specific Tables" key="DUMMY-MODEL-SPECIFIC-TABLES" type="Selection" default="NO" addon="ces">
            <option value="NO" name="No" />
            <option value="YES" name="Yes">
	        <variable name="Insert Data in USAP Frame Descriptor Table" key="STATS-DB-CES-USAP-FRAME-DESCRIPTOR-TABLE" type="Selection" default="NO" >
                    <option value="NO" name="No" />
                    <option value="YES" name="Yes"/>
                </variable>
            </option>
       </variable>
    </subcategory>   
</category>
