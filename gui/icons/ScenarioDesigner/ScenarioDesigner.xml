<application name="ScenarioDesigner" version="0.1" title="Scenario Designer" resource_dir="/gui/icons/ScenarioDesigner/">
    <actions>

        <!-- File Menu -->

        <menu name="ScenarioDesigner.MN.FileMenu" label="&amp;File" />
        

        <item name="ScenarioDesigner.MN.File.New" icon="icons/file_new_jd.png" statusTip="Create new scenario" menuText="&amp;New" accel="Ctrl+N" actSlot = "slotNewScenarioFile()"/>
        
        <item name="ScenarioDesigner.MN.File.Open" icon="icons/file_open_jd.png" statusTip="Open scenario file" menuText="&amp;Open" accel="Ctrl+O" actSlot = "slotOpenScenarioFile()"/>
        
        <menu name="ScenarioDesigner.MN.File.OpenRecentMenu" label="Recent Files" isDynamic = "true"/>
        
        <item name="ScenarioDesigner.MN.File.Close" statusTip="Close file" menuText="&amp;Close" actSlot = "slotCloseFile()"/>
        
        <item name="ScenarioDesigner.MN.File.CloseAll"  statusTip="Close all files" menuText="C&amp;lose All" actSlot = "slotCloseAllFiles()"/>
        
        <item name="ScenarioDesigner.MN.File.Save" icon="icons/save.png" statusTip="Save file" menuText="&amp;Save" accel="Ctrl+S" actSlot = "slotSaveScenarioFile()"/>
        
        <item name="ScenarioDesigner.MN.File.SaveAs" statusTip="Save file as" menuText="Sa&amp;ve As" actSlot = "slotSaveFileAs()"/>
        
        <item name="ScenarioDesigner.MN.File.SaveAsPortable" statusTip="Save All Associated files" menuText="Sav&amp;e As Portable" actSlot = "slotSaveFileAsPortable()"/>
        
        <item name="ScenarioDesigner.MN.File.SaveAll"  statusTip="Save all files" menuText="Save &amp;All" actSlot = "slotSaveAllFiles()"/>
        
        
        <item name="ScenarioDesigner.MN.File.PageSetup"  statusTip="Page setup" menuText="Page Se&amp;tup" accel="Ctrl+T" actSlot = "slotPageSetup()"/>
        
        <item name="ScenarioDesigner.MN.File.Print" icon="icons/print.png" statusTip="Print" menuText="&amp;Print" accel="Ctrl+P" actSlot = "slotPrint()"/>
        
        <item name="ScenarioDesigner.MN.File.Exit" statusTip="Exit" menuText="&amp;Exit" actSlot = "slotExit()"/>
        

        <ScenarioDesigner.MN.FileMenu>
            <ScenarioDesigner.MN.File.New />
            <ScenarioDesigner.MN.File.Open />
            <ScenarioDesigner.MN.File.OpenRecentMenu />
            <ScenarioDesigner.MN.File.Close />
            <ScenarioDesigner.MN.File.CloseAll />
            <separator />
            <ScenarioDesigner.MN.File.Save />
            <ScenarioDesigner.MN.File.SaveAs />
            <ScenarioDesigner.MN.File.SaveAsPortable />
            <ScenarioDesigner.MN.File.SaveAll />
            <separator />
            <ScenarioDesigner.MN.File.PageSetup />
            <ScenarioDesigner.MN.File.Print />
            <separator />
            <ScenarioDesigner.MN.File.Exit />
        </ScenarioDesigner.MN.FileMenu>


        <!-- Edit Menu -->

        <menu name="ScenarioDesigner.MN.EditMenu" label="&amp;Edit" />


        <item name="ScenarioDesigner.MN.Edit.Cut" icon="icons/cut.png" statusTip="Cut" menuText="Cu&amp;t" accel="Ctrl+X" actSlot = "slotCut()"/>
        
        <item name="ScenarioDesigner.MN.Edit.Copy" icon="icons/copy.png" statusTip="Copy" menuText="&amp;Copy" accel="Ctrl+C" actSlot = "slotCopy()"/>
        
        <item name="ScenarioDesigner.MN.Edit.Paste" icon="icons/paste.png" statusTip="Paste" menuText="&amp;Paste" accel="Ctrl+V" actSlot = "slotPaste()"/>
        
        <item name="ScenarioDesigner.MN.Edit.Delete" icon="icons/delete.png" statusTip="Delete" menuText="&amp;Delete" accel="Delete" actSlot = "slotDelete()"/>
    
        <item name="ScenarioDesigner.MN.Edit.SelectAll" icon="selectall.png" statusTip="Select all" menuText="Select &amp;All" accel="Ctrl+A" actSlot = "slotSelectAll()"/>

        <ScenarioDesigner.MN.EditMenu>
            <ScenarioDesigner.MN.Edit.Cut />
            <ScenarioDesigner.MN.Edit.Copy />
            <ScenarioDesigner.MN.Edit.Paste/>
            <ScenarioDesigner.MN.Edit.Delete/>
            <ScenarioDesigner.MN.Edit.SelectAll/>
        </ScenarioDesigner.MN.EditMenu>


    <!-- View  Menu -->
        <menu name="ScenarioDesigner.MN.ViewMenu" label="&amp;View" />
        <item name="ScenarioDesigner.MN.View.DisplayProperties" icon="display_properties.png" statusTip="Display Settings" menuText="&amp;Display Settings..." actSlot = "slotDisplayOptions()"/>

        <ScenarioDesigner.MN.ViewMenu>
            <ScenarioDesigner.MN.View.DisplayProperties />
        </ScenarioDesigner.MN.ViewMenu>


    
        <!-- Tools Menu -->

        <menu name="ScenarioDesigner.MN.ToolsMenu" label="&amp;Tools" />
        
        <item name="ScenarioDesigner.MN.Tools.NodePlacement" icon="none.png" statusTip="Node placement" menuText="&amp;Node Creation Wizard..." actSlot = "slotNodePlacement()"/>
            <item name="ScenarioDesigner.MN.Tools.NodePlacementManager" icon="none.png" statusTip="Node placement Manager" menuText="&amp;Node Placement Manager..." actSlot = "slotNodePlacementManager()"/> 
        
        <!-- <menu name="ScenarioDesigner.MN.Tools.SynchronizerMenu" label="Synchronizer" isDynamic = "false"/> -->
        
        <item name="ScenarioDesigner.MN.Tools.RecordTrace"  statusTip="Record trace" menuText="&amp;Record Trace" actSlot = "slotRecordTrace()"/>
        
        <item name="ScenarioDesigner.MN.Tools.RunSettings"  statusTip="Run settings" menuText="Run &amp;Settings..." actSlot = "slotRunSettings()"/>
        
        <item name="ScenarioDesigner.MN.Tools.Visualize"  statusTip="Visualize" menuText="&amp;Visualize" actSlot = "slotVisualize()"/>
        
        <item name="ScenarioDesigner.MN.Tools.NetworkObject" icon="newdevice.png" statusTip="Network Object Model" menuText="Device Model &amp;Editor..." actSlot = "slotNetworkObjectModelEditor()"/>
        
        <item name="ScenarioDesigner.MN.Tools.HierarchyModel" icon="newdevice.png" statusTip="Hierarchy Model" menuText="Hierarchy Model &amp;Editor..." actSlot = "slotHierarchyModelEditor()"/>
        
        <item name="ScenarioDesigner.MN.Tools.CustomizeToolset" icon="newdevice.png" statusTip="Toolset Editor" menuText="&amp;Toolset Editor..." actSlot = "slotCustomizeToolsetEditor()"/>        
        
        <!--<item name="ScenarioDesigner.MN.Tools.IpneEditor" icon="newdevice.png" statusTip="IPNE Config" menuText="&amp;IPNE Properties..." actSlot = "slotIpneWizard()"/>-->
        
        <!--<item name="ScenarioDesigner.MN.Tools.WeatherEditor" icon="newdevice.png" statusTip="Weather Config" menuText="&amp;Weather Properties..." actSlot = "slotWeatherEditor()"/>-->
        
        <item name="ScenarioDesigner.MN.Tools.CreateGroupMulticast" icon="none.png" statusTip="Multicast" menuText="&amp;Multicast Editor..." actSlot = "slotCreateGroupMulticast()"/> 
        
        <!--<menu name="ScenarioDesigner.MN.Tools.GroupEditorMenu" label="Group Editor" isDynamic = "false"/> -->
        
        <item name="ScenarioDesigner.MN.Tools.Settings" icon="settings.png" statusTip="Scenario properties" menuText="&amp;Scenario Properties..." actSlot = "slotGlobalSettings()"/>

        <ScenarioDesigner.MN.ToolsMenu>
            <ScenarioDesigner.MN.Tools.RunSettings/>
            <ScenarioDesigner.MN.Tools.Visualize/>
    
            <separator />
            <ScenarioDesigner.MN.Tools.SynchronizerMenu/>
            <ScenarioDesigner.MN.Tools.IpneEditor/>
            <ScenarioDesigner.MN.Tools.WeatherEditor/>
    
            <separator />
            <ScenarioDesigner.MN.Tools.RecordTrace />
    
            <separator />
            <ScenarioDesigner.MN.Tools.NodePlacement/>
            <ScenarioDesigner.MN.Tools.NodePlacementManager/>
            <ScenarioDesigner.MN.Tools.NetworkObject/>
            <ScenarioDesigner.MN.Tools.HierarchyModel/>
            <ScenarioDesigner.MN.Tools.CustomizeToolset/>
            <ScenarioDesigner.MN.Tools.CreateGroupMulticast/>
    
            <separator />
            <ScenarioDesigner.MN.Tools.Settings/>
        </ScenarioDesigner.MN.ToolsMenu>

