#
# Define Developer library models.
#

include ../addons/db/Makefile-common

ADDON_OPTIONS   = $(ADDON_OPTIONS) $(DATABASE_OPTIONS)
ADDON_SRCS      = $(ADDON_SRCS) $(DATABASE_SRCS)

ADDON_INCLUDES  = $(ADDON_INCLUDES) $(DATABASE_INCLUDES)

ADDON_LIBRARIES = $(ADDON_LIBRARIES) $(DATABASE_LIBRARIES)

ADDONS_CLEAN = $(ADDONS_CLEAN) dbclean
dbclean:
  cd $(DATABASE_SRCDIR) & del /s *.obj
